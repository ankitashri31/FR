<!-- Page header -->
<input type="hidden" id="hdnDocumentId" name="hdnDocumentId" />
<div class="page-header page-header-light">
    <div class="page-header-content header-elements-md-inline">
        <div class="page-title d-flex">
            <h4> <span class="font-weight-semibold">Tickets</span></h4>
            <a href="Dumy/index" class="header-elements-toggle text-default d-md-none"><i class="icon-more"></i></a>
        </div>
    </div>
    <!-- Page header -->

</div>
<!-- /page header -->
<!-- Content area -->

<div class="content">
    <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" id="Active-tab" data-toggle="tab" href="#Active" role="tab" aria-controls="Active"
               aria-selected="true">Active</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="Closed-tab" data-toggle="tab" href="#Closed" role="tab" aria-controls="Closed"
               aria-selected="false">Closed</a>
        </li>
    </ul>
    <a class="cke_button" asp-action="AddNewTicket" asp-controller="Dumy" style="float:right" ><i class="icon-plus2 mr-1"></i> Add New Ticket</a>
    
    <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="Active" role="tabpanel" aria-labelledby="Active-tab">
            <div class="card-body">
                <div class="row col-12">
                    <div class="col-2">
                        <div class="form-group">
                            @Html.DisplayNameFor(x => x.Channel)
                            @Html.DropDownListFor(x => Model.Channel, Model.ChannelList.OrderBy(x => x.Text), "--Select Channel--", htmlAttributes: new { @class = "form-control mt-2  f-14" })
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            @Html.DisplayNameFor(x => x.WaitingOn)
                            @Html.DropDownListFor(x => Model.WaitingOn, Model.WaitingOnList.OrderBy(x => x.Text), "--Select Waiting On--", htmlAttributes: new { @class = "form-control mt-2  f-14" })
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="form-group">
                            @Html.DisplayNameFor(x => x.SearchDate)
                            @Html.TextBoxFor(x => x.SearchDate, "{0:d MMM yyyy}", new { @class = "form-control" })
                        </div>

                    </div>
                    <div class="col-4">
                        <div class="form-group">
                            @Html.DisplayNameFor(x => x.Search)
                            @Html.TextBoxFor(x => x.Search, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="col-2">
                        <button type="submit" class="btn btn-primary" title="Submit" style="margin-top:25px;"><i class="icon-plus2 mr-1"></i> Search</button>
                    </div>
                </div>
                <table id="TableVoiData" class="table table-bordered table-striped dataTable no-footer">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                            <th>Channel</th>
                            <th>Waiting on</th>
                            <th>AddedBy</th>
                            <th>AddedOn</th>
                            <th class="text-center">Actions</th>
                        </tr>
                    </thead>

                </table>
            </div>
        </div>
