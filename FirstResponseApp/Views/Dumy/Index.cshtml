@model FirstResponseApp.Helper.Matter
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



@*DatePicker Query start
<script src="~/js/Admin/jquery-ui.js"></script>*@
@*DatePicker Query end*@


<script src="~/js/DataTable/Datatable.min.js"></script>
